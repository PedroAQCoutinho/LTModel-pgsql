--Building complete landtenure dataset
INSERT INTO recorte.v_pacotes_proc05_imoveisfull
    SELECT * FROM recorte.v_pacotes_proc03_unregistered AS a
    UNION ALL
    SELECT * FROM recorte.v_pacotes_proc04_flagprop AS b;
